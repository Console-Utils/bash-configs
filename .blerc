bleopt canvas_winch_action=redraw-prev

ble-face syntax_default=none
ble-face syntax_command='fg=cyan,bold'
ble-face syntax_quoted='fg=yellow'
ble-face syntax_quotation='fg=olive'
ble-face syntax_escape='fg=teal'
ble-face syntax_expr='fg=cyan'
ble-face syntax_error='fg=red'
ble-face syntax_varname='fg=cyan'
ble-face syntax_delimiter='fg=green'
ble-face syntax_param_expansion='fg=teal'
ble-face syntax_history_expansion='fg=brown'
ble-face syntax_function_name='fg=cyan,bold'
ble-face syntax_comment='fg=green'
ble-face syntax_glob='fg=teal'
ble-face syntax_brace='fg=teal'
ble-face syntax_tilde='fg=teal'
ble-face syntax_document_begin='fg=teal'
ble-face command_builtin_dot='fg=cyan,bold'
ble-face command_builtin='fg=cyan,bold'
ble-face command_alias='fg=cyan,bold,underline'
ble-face command_function='fg=cyan,bold'
ble-face command_file='fg=cyan,bold'
ble-face command_keyword='fg=blue,bold'
ble-face command_jobs='fg=red'
ble-face command_directory='fg=cyan,underline'
ble-face argument_option='fg=cyan'
ble-face varname_unset='fg=red'
ble-face varname_export='fg=cyan'
ble-face varname_array='fg=cyan'
ble-face varname_hash='fg=cyan'
ble-face varname_number='fg=cyan'
ble-face varname_readonly='fg=cyan'
ble-face varname_transform='fg=cyan'
ble-face varname_empty='fg=cyan'
ble-face varname_expr='fg=cyan'
